<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  <filter>
    <filter-name>AuthFilter</filter-name>
    <filter-class>controller.FilterSession</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AuthFilter</filter-name>
    <servlet-name>MainPage</servlet-name>
  </filter-mapping>
  <!--Servlets-->

  <!--SignUp-->

  <servlet>
    <servlet-name>SignUp</servlet-name>
    <servlet-class>controller.SignUp</servlet-class>
  </servlet>
  <!--SignIn-->
  <servlet>
    <servlet-name>SignIn</servlet-name>
    <servlet-class>controller.SignIn</servlet-class>
  </servlet>
  <!--MainPage-->
  <servlet>
    <servlet-name>MainPage</servlet-name>
    <servlet-class>controller.MainPage</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogOut</servlet-name>
    <servlet-class>controller.SignOut</servlet-class>
  </servlet>

  <!--Mapping-->
  <!--SignUp-->

  <servlet-mapping>
    <servlet-name>SignUp</servlet-name>
    <url-pattern>/signUp</url-pattern>
  </servlet-mapping>
  <!--SignIn-->
  <servlet-mapping>
    <servlet-name>SignIn</servlet-name>
    <url-pattern>/signIn</url-pattern>
  </servlet-mapping>
  <!--MainPage-->
  <servlet-mapping>
    <servlet-name>MainPage</servlet-name>
    <url-pattern>/mainPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogOut</servlet-name>
    <url-pattern>/signOut</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

</web-app>

